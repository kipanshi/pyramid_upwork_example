[app:main]
use = egg:pyramid_upwork_example
pyramid.reload_templates = true
pyramid.includes =
    pyramid_jinja2
    pyramid_debugtoolbar

# Redis session settings
redis.sessions.secret = some secret

# upwork settings
upwork.api.key = some consumer key
upwork.api.secret = some consume key secret

[server:main]
use = egg:pyramid#wsgiref
host = 0.0.0.0
port = 6543

# Begin logging configuration

[loggers]
keys = root, pyramid_upwork_example

[logger_pyramid_upwork_example]
level = DEBUG
handlers =
qualname = pyramid_upwork_example

[handlers]
keys = console

[formatters]
keys = generic

[logger_root]
level = INFO
handlers = console

[handler_console]
class = StreamHandler
args = (sys.stderr,)
level = NOTSET
formatter = generic

[formatter_generic]
format = %(asctime)s %(levelname)-5.5s [%(name)s][%(threadName)s] %(message)s

# End logging configuration